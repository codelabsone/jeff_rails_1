<h1>Create Automotive</h1>

<%= form_for(@inventory.make) do |f| %>
<p>
  <%= f.label :make %><br/>
  <%= f.text_field :make %>
</p>
<p>
  <%= f.label :model %><br/>
  <%= f.text_area :model %>
</p>
<p>
  <%= f.label :color %><br/>
  <%= f.text_field :color %>
</p>
<p>
  <%= f.label :year %><br/>
  <%= f.date_select :year %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>
