<h1>Editing Inventory:</h1>

<%= form_for(@inventory) do |f| %>
<p>
  <%= f.label :make %><br/>
  <%= f.text_field :make %>
</p>
<p>
  <%= f.label :model %><br/>
  <%= f.text_field :model %>
</p>
<p>
  <%= f.label :color %><br/>
  <%= f.text_field :color %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>
